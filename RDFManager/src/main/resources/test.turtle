@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://localhost/SensorSchema/ontology> a owl:Ontology .
<obs:hasArea> a owl:AnnotationProperty .
<obs:hasDistance> a owl:AnnotationProperty .
<obs:hasObservation> a owl:ObjectProperty .
<obs:hasDateTime>
  a owl:DatatypeProperty ;
  rdfs:range xsd:dateTime .

<obs:hasAcceleration>
  a owl:DatatypeProperty ;
  rdfs:range xsd:double .

<obs:hasDate>
  a owl:DatatypeProperty ;
  rdfs:range xsd:integer .

<obs:hasDirection>
  a owl:DatatypeProperty ;
  rdfs:range xsd:int .

<obs:hasDuration>
  a owl:DatatypeProperty ;
  rdfs:range xsd:long .

<obs:hasEndLatitude>
  a owl:DatatypeProperty ;
  rdfs:subPropertyOf <obs:hasEndLocation> .

<obs:hasEndLocation> a owl:DatatypeProperty .
<obs:hasEndLongitude>
  a owl:DatatypeProperty ;
  rdfs:subPropertyOf <obs:hasEndLocation> .

<obs:hasID>
  a owl:DatatypeProperty ;
  rdfs:range xsd:integer .

<obs:hasLatitude>
  a owl:DatatypeProperty ;
  rdfs:subPropertyOf <obs:hasLocation> ;
  rdfs:range xsd:double .

<obs:hasLocation> a owl:DatatypeProperty .
<obs:hasLongitude>
  a owl:DatatypeProperty ;
  rdfs:subPropertyOf <obs:hasLocation> ;
  rdfs:range xsd:double .

<obs:hasSender>
  a owl:DatatypeProperty ;
  rdfs:range xsd:int .

<obs:hasStartLatitude>
  a owl:DatatypeProperty ;
  rdfs:subPropertyOf <obs:hasStartLocation> .

<obs:hasStartLocation> a owl:DatatypeProperty .
<obs:hasStartLongitude>
  a owl:DatatypeProperty ;
  rdfs:subPropertyOf <obs:hasStartLocation> .

<obs:hasVelocity>
  a owl:DatatypeProperty ;
  rdfs:range xsd:double .

<http://www.w3.org/2003/11/swrlb#args> a owl:DatatypeProperty .
<http://www.w3.org/2003/11/swrlb#maxArgs> a owl:DatatypeProperty .
<http://www.w3.org/2003/11/swrlb#minArgs> a owl:DatatypeProperty .
<obs:Byway>
  a owl:Class ;
  rdfs:label "Byway"@en ;
  rdfs:subClassOf <obs:Road> .

<obs:Crossing>
  a owl:Class ;
  rdfs:subClassOf <obs:Event> .

<obs:Event> a owl:Class .
<obs:HighAcceleration>
  a owl:Class ;
  rdfs:subClassOf <obs:Event> ;
  rdfs:comment ""^^xsd:string .

<obs:Acceleration>
  a owl:Class ;
  rdfs:subClassOf <obs:Event> ;
  rdfs:comment ""^^xsd:string .

<obs:HighAvgSpeed>
  a owl:Class ;
  rdfs:subClassOf <obs:Speed> .

<obs:HighAvgSpeed1>
  a owl:Class ;
  rdfs:subClassOf <obs:Speed> .

<obs:HighAvgSpeed2>
  a owl:Class ;
  rdfs:subClassOf <obs:Speed> .

<obs:HighAvgSpeed3>
  a owl:Class ;
  rdfs:subClassOf <obs:Speed> .

<obs:HighAvgSpeed4>
  a owl:Class ;
  rdfs:subClassOf <obs:Speed> .

<obs:HighDeacceleration>
  a owl:Class ;
  rdfs:subClassOf <obs:Event> .

<obs:HighSpeed>
  a owl:Class ;
  rdfs:subClassOf <obs:Speed> .

<obs:Highway>
  a owl:Class ;
  rdfs:label "Highway"@en ;
  rdfs:subClassOf <obs:Road> .

<obs:HotSpot>
  a owl:Class ;
  rdfs:subClassOf <obs:Event> .

<obs:Jam>
  a owl:Class ;
  rdfs:subClassOf <obs:Event>, [
    a owl:Restriction ;
    owl:onProperty <obs:hasObservation> ;
    owl:allValuesFrom <obs:Observation>
  ] .

<obs:JamSpeed1>
  a owl:Class ;
  rdfs:subClassOf <obs:Speed>, [
    a owl:Restriction ;
    owl:onProperty <obs:hasObservation> ;
    owl:allValuesFrom <obs:Observation>
  ] .

<obs:JamSpeed2>
  a owl:Class ;
  rdfs:subClassOf <obs:Speed> .

<obs:JamSpeed3>
  a owl:Class ;
  rdfs:subClassOf <obs:Speed> .

<obs:JamSpeed4>
  a owl:Class ;
  rdfs:subClassOf <obs:Speed> .

<obs:JamSpeed5>
  a owl:Class ;
  rdfs:subClassOf <obs:Speed> .

<obs:JamZone>
  a owl:Class ;
  rdfs:subClassOf <obs:Event> .

<obs:LeftTurn>
  a owl:Class ;
  rdfs:subClassOf <obs:Turn> .

<obs:LongStop>
  a owl:Class ;
  rdfs:subClassOf <obs:Stop> .

<obs:LowAvgSpeed>
  a owl:Class ;
  rdfs:subClassOf <obs:Speed> .

<obs:LowSpeed>
  a owl:Class ;
  rdfs:subClassOf <obs:Speed> .

<obs:MidAvgSpeed>
  a owl:Class ;
  rdfs:subClassOf <obs:Speed> .

<obs:Motorway>
  a owl:Class ;
  rdfs:subClassOf <obs:Road> .

<obs:Observation> a owl:Class .
<obs:RightTurn>
  a owl:Class ;
  rdfs:subClassOf <obs:Turn> .

<obs:Road>
  a owl:Class ;
  rdfs:label "Road"@en .

<obs:SlowTrafficFlow>
  a owl:Class ;
  rdfs:subClassOf <obs:Event> .

<obs:Speed>
  a owl:Class ;
  rdfs:subClassOf <obs:Event> .

<obs:Stop>
  a owl:Class ;
  rdfs:subClassOf <obs:Event> .

<obs:Stop1>
  a owl:Class ;
  rdfs:subClassOf <obs:Stop> .

<obs:Stop2>
  a owl:Class ;
  rdfs:subClassOf <obs:Stop> .

<obs:Stop3>
  a owl:Class ;
  rdfs:subClassOf <obs:Stop> .

<obs:Stop4>
  a owl:Class ;
  rdfs:subClassOf <obs:Stop> .

<obs:StopOnMotorway>
  a owl:Class ;
  rdfs:subClassOf <obs:Event> .

<obs:TrafficJam>
  a owl:Class ;
  rdfs:subClassOf <obs:Jam> .

<obs:Turn>
  a owl:Class ;
  rdfs:subClassOf <obs:Event> .

<obs:UTurn>
  a owl:Class ;
  rdfs:subClassOf <obs:Turn> .

<obs:UTurnZone>
  a owl:Class ;
  rdfs:subClassOf <obs:Event> .

<obs:VeryLongStop>
  a owl:Class ;
  rdfs:subClassOf <obs:Stop> .

[] a [
    a owl:Restriction ;
    owl:onProperty <obs:hasVelocity> ;
    owl:allValuesFrom xsd:anyURI
  ] .